package com.score3;

import java.util.Scanner;

public class ScoreImpl implements Score {

	private int w, h;
	private Record rec;
	
	Scanner sc = new Scanner(System.in);

	@Override
	public void set() {

		System.out.println("가로?");
		w = sc.nextInt();
		System.out.println("세로?");
		h = sc.nextInt();

	}

	@Override
	public void input() {
		
		area = w * h;
	}

	@Override
	public void print() {
	}

}
