package com.day2;

import java.util.Scanner;

public class Q6 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		int a,b,c,d;
		String name,str;
		
		System.out.print("이름?");
		System.out.print("3과목 점수?");
		name = sc.next();
		a = sc.nextInt();
		b = sc.nextInt();
		c = sc.nextInt();
		d = sc.nextInt();
		
		
		if(a<40 || b<40 || c<40){
			System.out.println("과락");
		}if else(d<60){
			System.out.println("불합격");
		}else {
			System.out.println("합격");
		}
	}

}
