package com.rank;

import java.util.Scanner;

class Record {

	String name;
	int[] score;
}

class Score {

	Scanner sc = new Scanner(System.in);
	Record re = new Record();
	int inwon, i, j;

	public void set() {

		do {
			System.out.println("인원을 입력하세요.");
			inwon = sc.nextInt();
		} while (inwon < 1 || inwon > 10);
		re = new Record[inwon];
	}

	public void input() {
	
		String[] title = { "국어", "영어", "수학" };
		
		for(i=0;i<inwon;i++) {

			re[i] = new Record();
			
			
			
		System.out.println("이름을 입력하세요");

		System.out.println("");

	}

}

