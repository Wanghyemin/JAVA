package com.score3;

public class ScoreVO { //Value Object : 데이터를 저장

	private String hak;
	private String name;
	private int kor;
	private int eng;
	private int mat;
	
	public ScoreVO() {
		
		
	}
	public ScoreVO(String hak, String name, int kor, int eng, int mat) {
		
		this.hak = hak;
		this.name = name;
		this.kor = kor;
		this.eng = eng;
		this.mat = mat;
				
		
		
	}
	
	
	
	
	
	
	
	
	
}
