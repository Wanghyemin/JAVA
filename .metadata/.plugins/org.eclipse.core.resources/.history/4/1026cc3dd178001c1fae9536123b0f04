package com.project;

public class ThreadSrt extends Thread {

	@Override
	public void run() {

		try {

			String print = "¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à\r\n"
					+ "¡à¡à¡á¡á¡á¡à¡à¡á¡á¡á¡á¡á¡à¡á¡à¡à¡à¡à¡à¡á¡à¡á¡á¡á¡à¡á¡á¡á¡á¡à¡à¡à¡à¡à¡á¡à¡à¡à¡à¡á¡á¡á¡à¡à\r\n"
					+ "¡à¡á¡à¡à¡à¡á¡à¡á¡à¡à¡à¡á¡à¡á¡à¡à¡à¡à¡à¡á¡à¡à¡á¡à¡à¡á¡à¡à¡à¡á¡à¡à¡à¡á¡á¡à¡à¡à¡á¡à¡à¡à¡á¡à\r\n"
					+ "¡à¡á¡à¡à¡à¡à¡à¡á¡à¡à¡à¡á¡à¡á¡à¡à¡à¡à¡à¡á¡à¡à¡á¡à¡à¡á¡à¡à¡à¡á¡à¡à¡á¡à¡á¡à¡à¡à¡á¡à¡à¡à¡á¡à\r\n"
					+ "¡à¡á¡à¡à¡à¡à¡à¡á¡à¡à¡à¡á¡à¡á¡à¡à¡à¡à¡à¡á¡à¡à¡á¡à¡à¡á¡à¡à¡à¡á¡à¡à¡à¡à¡á¡à¡à¡à¡à¡á¡á¡á¡á¡à\r\n"
					+ "¡à¡á¡à¡à¡à¡à¡à¡á¡à¡à¡à¡á¡à¡à¡á¡à¡à¡à¡á¡à¡à¡à¡á¡à¡à¡á¡à¡à¡à¡á¡à¡à¡à¡à¡á¡à¡à¡à¡à¡à¡à¡à¡á¡à\r\n"
					+ "¡à¡á¡à¡à¡à¡á¡à¡á¡à¡à¡à¡á¡à¡à¡à¡á¡à¡á¡à¡à¡à¡à¡á¡à¡à¡á¡à¡à¡à¡á¡à¡à¡à¡à¡á¡à¡à¡à¡á¡à¡à¡à¡á¡à\r\n"
					+ "¡à¡à¡á¡á¡á¡à¡à¡á¡á¡á¡á¡á¡à¡à¡à¡à¡á¡à¡à¡à¡à¡á¡á¡á¡à¡á¡á¡á¡á¡à¡à¡à¡á¡á¡á¡á¡á¡à¡à¡á¡á¡á¡à¡à\r\n"
					+ "¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à¡à\r\n";

			System.out.println(print);
			
			sleep(100);

			for (int i = 0; i < print.length(); i++) {

				String ch = print.substring(i, i + 1);
				if (ch.equals("¡á")) {
					ch = "¡à";
				} else if (ch.equals("¡à")) {
					ch = "¡á";
				}
				
				print = print + ch;
			}
			
			System.out.println(print);
			
			sleep(100);

			

		} catch (Exception e) {

		}

	}
}
