package com.day14;

import java.util.Scanner;

public class Why {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		System.out.println("입력");

		String st;

		do {
			System.out.println("성별? (a or b)");
			st = sc.next();
		} while ((st != "남") && (st != "여"));

	}

}