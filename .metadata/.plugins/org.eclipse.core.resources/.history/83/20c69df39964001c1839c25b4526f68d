package com.day3;

import java.util.Scanner;

public class Q4 {

	public static void main(String[] args) {

/*		구구단을 출력하세요*/
			
		Scanner sc = new Scanner(System.in);
		
		int i,j;
		
		for (i=2;i<=5;i++) {
			for(j=1;j<=9;j++) {
				System.out.printf("%d * %d = %d",i,j,i*j);
			}
			
		}
		
		
		
		
	}

}
