package com.day13;

import java.util.ArrayList;
import java.util.Iterator;

public class Test1 {

	public static void main(String[] args) {

		ArrayList<String> lists = new ArrayList<>();
		
		lists.add("서울");
		lists.add("부산");
		lists.add("대구");
		
		Iterator<String> it = lists.iterator();
		while(it.hasNext()) {
			
			String str = it.next();
			System.out.print(str + " ");
		}
		System.out.println();
		
		
		
		
	}

}
