package com.day6;

class SubClass{
	
	int a; // instance 변수 (전역 변수)
	int b;
	

}



public class Test1 {

	public static void main(String[] args) {
	
		SubClass ob1 = new SubClass();
		SubClass ob2 = new SubClass();
		
		System.out.println("ob1: "+ob1.a);
		System.out.println("ob1: "+ob1.b);
		
		ob1.a = 10;
		ob1.b = 20;
		
		
	}
}
