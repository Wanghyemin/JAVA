package com.day15;

class MyThread2 implements Runnable{

	private int num;
	private String name;
	
	public MyThread2(int num,String name) {
		
		this.num = num;
		this.name = name;
		
	}
	
	@Override
	public void run() {
		
		while(i<num){
			System.out.println(name + ":"+i);
			i++;
			try {
				Thread.sleep(100);
			} catch (Exception e) {
				// TODO: handle exception
		
				
				
			
		}
		
		
	}
	
	
	
}


