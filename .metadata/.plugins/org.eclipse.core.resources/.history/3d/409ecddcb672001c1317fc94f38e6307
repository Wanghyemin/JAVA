package com.day15;

import java.util.Random;
import java.util.Scanner;

public class ClassRandom {

	private static String[] name = 
		{"전상원","박  범","김찬주","진현주","남승은",
		"박가윤","서동혁","양세리","김창훈","이주현",
		"왕혜민","이종원","장하은","김성제","채현우",
		"이윤재","전성덕","이나영","배승환","이한솔"};
	
	public void ClassRandom(String[] name) {
		this.name = name;
	}
	
	public void input() {
		
		Random rd = new Random();
		Scanner sc = new Scanner(System.in);
		
		int i,j,inwon,n;
		
		System.out.println("발표자 인원수?");
		inwon = sc.nextInt();
		
		while(n<inwon) {
			name[n] = rd.next(n)+1;
			
			for(i = 0;i<n;i++) {
				
				if(name[i].equals(name[n])) {
					n--;
					break;
				}
			}
			n++;
		}
		
		
	
	}
	
	public static void main(String[] args) {
		


		
		
		
	}
}
