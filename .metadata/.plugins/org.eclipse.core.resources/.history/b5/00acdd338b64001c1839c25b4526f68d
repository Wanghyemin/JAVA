package com.day3;

public class Test8 {

	public static void main(String[] args) {

		//배열
		//int num[] = new int[5]
		double[] num = new double[5];
		
		System.out.println("배열 갯수: "+ num.length);
		
		num[0] = 10;
		num[1] = 20;
		num[2] = 30;
		num[3] = 40;
		num[4] = 50;
		
		
		
		for(int i = 1; i <=num.length-1;i++) {
		
			System.out.println("num[i]: "+ num[i])''
		
	}

}
