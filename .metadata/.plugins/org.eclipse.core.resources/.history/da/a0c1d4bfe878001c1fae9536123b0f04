		System.out.println(print);

	}
	
	public void xstring() {	// COVID 19 그림 반전 프린트

		String xprint = "";
		for (int i = 0; i < print.length(); i++) {

			String str = print.substring(i, i + 1);
			
			if (str.equals("■")) {
				str = "□";
			} else if (str.equals("□")) {
				str = "■";
			}
			xprint += str;
		}

		System.out.println(xprint);
	}

	
	@Override
	public void run() {

		try {
			
			string();
			sleep(500);
			
			xstring(); 
			sleep(500);

		} catch (Exception e) {

		}

		
