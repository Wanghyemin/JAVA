package com.day9;

class TestA {

	private int a = 10;

	public void write() {

		System.out.println("a: " + a);
	}

}

public class Test2 {

	public static void main(String[] args) {
		
		TestA ob1 = new TestA();
		TestA ob2 = new TestA();
		
		System.out.println("ob1 == ob2: " + (ob1==ob2)); //false : 주소지 비교
		
		System.out.println("ob1.equals(ob2): " + ob2.equals(ob2));
		
		
		
		
		
		
		

	}

}
