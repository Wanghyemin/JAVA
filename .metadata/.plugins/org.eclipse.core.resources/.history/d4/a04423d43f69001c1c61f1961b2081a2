package com.day7;

public class Test7 {
	
	int a = 5; //instance변수
	
	{ //초기화 블럭
		System.out.println("초기화 블럭 a: " + a);
		a = 10;
		System.out.println("초기화 블럭 a: " + a);
	}

	static int b;
	static { //static 블럭
		b = 10;
		System.out.println("static 블럭 b: " + b);
	}
	
	// 변수: 변하는 수
	// 상수: 항상 같은 값
	final int C=0; // 상수
	
	
	
	
	public static void main(String[] args) {

	}

}
