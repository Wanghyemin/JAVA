package com.day3;

import java.util.Scanner;

public class Test8 {

	public static void main(String[] args) {

		// 배열
		// int num[] = new int[5]
		int[] num = new int[5];

		Scanner sc = new Scanner(System.in);

		System.out.print("다섯개의 변수?");//5 7 9 2 4
		
		num[0] = sc.nextInt();
		num[1] = sc.nextInt();
		num[2] = sc.nextInt();
		num[3] = sc.nextInt();
		num[4] = sc.nextInt();

		for (int i = 0; i < num.length; i++) {
			System.out.println("num[" + i + "]: " + num[i]);
		}
		
		System.out.println("배열 갯수: " + num.length);
		
		
	}
}
